{
  "name": "raf-mapa-producao-server",
  "version": "1.0.0",
  "description": "Backend server for RAF Mapa de Produção",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:all": "npm run type-check && npm run test && npm run test:e2e",
    "postinstall": "prisma generate --schema=./src/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./src/prisma/schema.prisma",
    "seed": "tsx src/scripts/seed-templates.ts",
    "set-admin": "tsx src/scripts/set-admin.ts",
    "fix-credential-accounts": "tsx src/scripts/fix-credential-accounts.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.16.0",
    "pdf-parse": "^1.1.4",
    "pdfjs-dist": "^5.4.530",
    "pino": "^10.3.0",
    "sharp": "^0.34.5",
    "tesseract.js": "^7.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.0",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^6.9.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
